var app = require('../app');
var http = require('http');

var port = 7070;
if(process.env.port != null){
  port = process.env.port;
}
console.log(process.env.port);
app.set('port', port);

var server = http.createServer(app);

server.listen(port);
server.on('listening', () => {
   console.log("Listening on %s", server.address().port);
});
